{
  "name": "moneyd-web-payments",
  "version": "1.0.0",
  "description": "Package to run the moneyd-web-payment sender and receiver",
  "main": "index.js",
  "scripts": {
    "start": "npm run build-dev && node ./distlib/src/index.js",
    "electron": "npm run build-dev && electron distlib/src/server/electron.js",
    "docker-debug": "npm run build && docker-compose build && docker-compose up",
    "dockerize": "npm run build && docker build -t cbhubb8/ilp-cloud-connector .",
    "build-dev": "npm run copy && tsc -p ./ && webpack --config ./distlib/webpack.config.js --mode development",
    "build": "npm run copy && tsc -p ./",
    "copy": "copyfiles src/**/*.json distlib/",
    "test": "npm run build && jest --coverage --collectCoverageFrom=src/**/*.{ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codius-smart-device-sharing/moneyd-web-payments.git"
  },
