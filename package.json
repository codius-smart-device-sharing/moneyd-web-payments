  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codius-smart-device-sharing/moneyd-web-payments/issues"
  },
  "homepage": "https://github.com/codius-smart-device-sharing/moneyd-web-payments#readme",
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "copyfiles": "^2.1.0",
    "get-port": "^4.2.0",
    "ilp": "^14.1.0",
    "ilp-plugin": "^3.2.1",
    "ilp-plugin-ethereum": "^3.0.0-beta.17",
    "ilp-plugin-xrp-asym-client": "^1.6.0",
    "ilp-plugin-xrp-paychan-shared": "^4.1.3",
    "is-docker": "^2.0.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-combine-routers": "^4.0.2",
    "koa-ilp": "^1.1.1",
    "koa-mount": "^4.0.0",
    "koa-request": "^1.0.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "localtunnel": "^1.9.2",
    "minimist": "^1.2.0",
    "node-fetch": "^2.6.1",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "^6.0.1",
